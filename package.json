{
  "name": "shopify-front-boilerplate",
  "version": "0.0.1",
  "description": "Boilerplate for the large Shopify repo",
  "main": "webpack.config.js",
  "scripts": {
    "test": "jest",
    "theme": "npx shopify-themekit deploy --env development",
    "theme:prod": "npx shopify-themekit deploy --env production",
    "lint": "npx eslint ./src",
    "lint:fix": "npx eslint ./src --fix",
    "deploy": "npm run lint && npm run theme",
    "deploy:prod": "npm run lint && npm run theme:prod"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kreezag/shopify-front-boilerplate.git"
  },
  "keywords": [
    "Shopify",
    "Boilerplate",
    "Codestyle",
    "Scss"
  ],
  "author": "Kreezag",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Kreezag/shopify-front-boilerplate/issues"
  },
  "homepage": "https://github.com/Kreezag/shopify-front-boilerplate#readme",
  "directories": {
    "doc": "docs"
  },
  "devDependencies": {
    "@babel/core": "^7.13.15",
    "@babel/plugin-transform-runtime": "^7.13.15",
    "@babel/preset-env": "^7.13.15",
    "@babel/runtime": "^7.13.10",
    "babel-loader": "^8.2.2",
    "copy-webpack-plugin": "^8.1.1",
    "css-loader": "^5.2.2",
    "eslint": "^7.26.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.1",
    "eslint-plugin-prettier": "^3.4.0",
    "imagemin-webpack-plugin": "^2.4.2",
    "jest": "^26.6.3",
    "mini-css-extract-plugin": "^1.4.1",
    "postcss": "^8.2.10",
    "postcss-loader": "^5.2.0",
    "prettier": "^2.3.0",
    "remove-files-webpack-plugin": "^1.4.4",
    "sass": "^1.32.10",
    "sass-loader": "^11.0.1",
    "style-loader": "^2.0.0",
    "webpack": "^5.33.2",
    "webpack-cli": "^4.6.0"
  },
  "dependencies": {
    "@shopify/themekit": "^1.1.7"
  }
}
